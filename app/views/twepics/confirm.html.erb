<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @twepic, url: twepics_path, local: true) do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>

  <p>タイトル:<%= @twepic.title %></p>
  <p>本文:<%= @twepic.content %></p>
  <%= form.submit  "登録する" %>
<% end %>

<%= form_with(model: @twepic, url: new_twepic_path, local: true, method: 'get') do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>
  <%= form.submit "もどる", name: 'back' %>
<% end %>
