<%= form_with(model: Twepic.new, local: true) do |form| %>
  <div class="twepic_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="twepic_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
<%= link_to '一覧画面にもどる', twepics_path %>
